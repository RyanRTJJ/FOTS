{
    "name": "name-of-folder-of-saved-models",
    "cuda": true,
    "gpus": [0, 1, 2, 3],
    "finetune": "",
    "need_grad_backbone": true,
    "data_loader": {
        "dataset":"mydataset",
        "image_dir": "/full/path/to/FOTS/data/MSCOCO/images",
        "annotation_dir": "/full/path/to/FOTS/data/MSCOCO/annotations",
        "batch_size": 4,
        "shuffle": true,
        "workers": 0
    },
    "validation": {
        "validation_split": 0.15,
        "shuffle": true
    },

    "lr_scheduler_type": "ExponentialLR",
    "lr_scheduler_freq": 50,
    "lr_scheduler": {
            "gamma": 0.94
    },

    "optimizer_type": "Adam",
    "optimizer": {
        "lr": 0.0001,
        "weight_decay": 1e-5
    },
    "loss": "FOTSLoss",
    "metrics": ["fots_metric"],
    "trainer": {
        "epochs": 10000,
        "save_dir": "/full/path/to/FOTS",
        "save_freq": 1,
        "verbosity": 2,
        "monitor": "loss",
        "monitor_mode": "min"
    },
    "arch": "FOTSModel",
    "model": {
        "mode": "united",
        "scale": 512,
        "crnn": {
            "img_h": 16,
            "hidden": 1024
        },
        "keys": "ryan_1"
    }
}
